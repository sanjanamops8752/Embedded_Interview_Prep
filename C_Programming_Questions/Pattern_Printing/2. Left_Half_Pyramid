//print stars 
#include <stdio.h> 

int main()
{
    for(int i=0;i<5;i++)
    {
        for(int j=0;j<2*(5-i-1);j++)
        {
            printf(" ");
        }
        for(int k=0;k<=i;k++)
        {
            printf("*");
        }
        printf("\n");
    }
}

//print numbers 
#include <stdio.h> 

int main()
{
    for(int i=0;i<5;i++)
    {
        for(int j=0;j<2*(5-i-1);j++)
        {
            printf(" ");
        }
        for(int k=0;k<=i;k++)
        {
            printf("%d",k+1);
        }
        printf("\n");
    }
}

//print letters
#include <stdio.h> 

int main()
{
    for(int i=0;i<5;i++)
    {
        for(int j=0;j<2*(5-i-1);j++)
        {
            printf(" ");
        }
        for(int k=0;k<=i;k++)
        {
            printf("%d",k+'A');
        }
        printf("\n");
    }
}
